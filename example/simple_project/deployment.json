{
    "externalDomain": "mongo-example.me",
    "prodBranch": "prod",
    "plugins": [
        {
            "kind": "mongoDB",
            "clusterName": "mongoDB",
            "prodDedicated": true,
            "reviewDedicated": false,
            "size": "SMALL"
        }
    ],
    "services": [
        {
            "image": "mongo-express",
            "version": "1.0.2-20-alpine3.19",
            "name": "mongo-express",
            "size": "SMALL",
            "externalDomainPrefix": "gui",
            "exposedPort": 8081,
            "vars": [
                {
                    "key": "ME_CONFIG_BASICAUTH",
                    "value": "false"
                }
            ],
            "plugins": [
                {
                    "kind": "mongoDB",
                    "clusterName": "mongoDB",
                    "databaseName": "testDB",
                    "right": "rw",
                    "exportedEnvMapping": {
                        "uri": "ME_CONFIG_MONGODB_URL"
                    }
                }
            ],
            "volumes": []
        }
    ]
}
